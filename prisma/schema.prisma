datasource db {
  provider = "postgresql" // or "mysql" or "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// model for  our user table in prisma 

model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  age Int
  isMarried Boolean
  nationality String
}
